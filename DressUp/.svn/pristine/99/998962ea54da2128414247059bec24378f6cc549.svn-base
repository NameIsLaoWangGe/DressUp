  "code": "import GameDataController from \"../GameDataController\";\r\nexport default class ActiveItem extends Laya.Script {\r\n    constructor() {\r\n        super(...arguments);\r\n        this.Datas = [];\r\n        this.MaxHeight = 85;\r\n        this.MaxWeight = 85;\r\n        this.str = {};\r\n    }\r\n    onAwake() {\r\n        let item = this.owner;\r\n        this.Bg = item.getChildByName(\"Bg\");\r\n        this.TaskPre = item.getChildByName(\"TaskPre\");\r\n        this.Now = this.TaskPre.getChildByName(\"TaskPre\");\r\n        this.Need = this.TaskPre.getChildByName(\"TaskPre\");\r\n        this.IconAll = item.getChildByName(\"IconAll\");\r\n        this.ClothShow = item.getChildByName(\"ClothShow\");\r\n        for (let i = 0; i < this.ClothShow.numChildren; i++) {\r\n            this.Icons.push(this.ClothShow.getChildAt(i).getChildAt(0));\r\n        }\r\n        this.ADBtn = item.getChildByName(\"ADBtn\");\r\n        this.ADBtn.on(Laya.Event.CLICK, this, this.adclick);\r\n    }\r\n    Fell(mes) {\r\n        this.Datas = mes;\r\n        this.Num = this.Datas[0].num;\r\n        this.Icons.forEach((v, i) => {\r\n            if (i < this.Datas.length) {\r\n                v.visible = true;\r\n                v.skin = this.Datas[i].GetPath1();\r\n                let imageHeight = parseFloat(v.height.toString());\r\n                let imagewidth = parseFloat(v.width.toString());\r\n                let pr = 0;\r\n                if (imagewidth > imageHeight) {\r\n                    pr = this.MaxWeight / imagewidth;\r\n                }\r\n                else {\r\n                    pr = this.MaxHeight / imageHeight;\r\n                }\r\n                v.scaleX = pr;\r\n                v.scaleY = pr;\r\n                v.centerX = 0;\r\n                v.centerY = 0;\r\n            }\r\n            {\r\n                v.visible = false;\r\n            }\r\n        });\r\n        for (let k in this.Datas) {\r\n            let v = GameDataController.ClothDataRefresh[k];\r\n            this.str[k] = v;\r\n        }\r\n        GameDataController.ClothdatapackSet(this.Datas[0].Type2, this.str);\r\n        this.Num = GameDataController.ClothAllUnlock(this.str);\r\n        this.Now.value = (this.Datas.length - this.Num) + \"\";\r\n        this.Need.value = (this.Datas.length) + \"\";\r\n        this.ADBtn.visible = this.Num > 0;\r\n    }\r\n    adclick() {\r\n        for (let k in this.str) {\r\n            if (this.str[k] == 1) {\r\n                this.str[k] = 0;\r\n                GameDataController.ClothdatapackSet(this.Datas[0].Type2, this.str);\r\n                return;\r\n            }\r\n        }\r\n    }\r\n}\r\n//# sourceMappingURL=ActiveItem.js.map",
