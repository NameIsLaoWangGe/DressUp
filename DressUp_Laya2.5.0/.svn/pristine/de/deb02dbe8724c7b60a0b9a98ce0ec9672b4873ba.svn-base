  "code": "import { UIBase, TweenMgr, UIMgr } from \"../../Frame/Core\";\r\nexport default class UIReady extends UIBase {\r\n    constructor() {\r\n        super(...arguments);\r\n        this.clothisopen = true;\r\n    }\r\n    onInit() {\r\n        this.windowheight = Laya.Browser.height;\r\n        this.windowwidth = Laya.Browser.width;\r\n        this.btnEv(\"ClothOpenBtn\", this.ClothOpenBtnClick);\r\n        this.ShowView = this.vars(\"ShowView\");\r\n        this.BtnBar = this.vars(\"BtnBar\");\r\n        this.ClothOpenBtn = this.vars(\"ClothOpenBtn\");\r\n        this.Ubag = TweenMgr.tweenCust(200, this, this.tweenbagUp, null, true, Laya.Ease.backOut);\r\n        this.Lbtn = TweenMgr.tweenCust(300, this, this.tweenbtnLeft, null, true, Laya.Ease.backOut);\r\n        this.btnU = TweenMgr.tweenCust(200, this, this.RoteUp, null, true, Laya.Ease.linearNone);\r\n        this.Dbag = TweenMgr.tweenCust(200, this, this.tweenbagDown, null, true, Laya.Ease.linearNone);\r\n        this.Rbtn = TweenMgr.tweenCust(300, this, this.tweenbtnRight, null, true, Laya.Ease.linearNone);\r\n        this.btnD = TweenMgr.tweenCust(200, this, this.RoteDown, null, true, Laya.Ease.linearNone);\r\n    }\r\n    buyFun() {\r\n    }\r\n    RefreshUI() {\r\n    }\r\n    onShow() {\r\n        this.RefreshUI();\r\n    }\r\n    onHide() {\r\n    }\r\n    ClothOpenBtnClick() {\r\n        if (this.clothisopen) {\r\n            console.log(\"消失\");\r\n            this.Dbag.play();\r\n            this.Rbtn.play();\r\n            this.btnU.play();\r\n        }\r\n        else {\r\n            console.log(\"展示\");\r\n            this.Lbtn.play();\r\n            this.Ubag.play();\r\n            this.btnD.play();\r\n        }\r\n        this.clothisopen = !this.clothisopen;\r\n    }\r\n    RoteDown(t) {\r\n        let nbtm = this.ClothOpenBtn.rotation;\r\n        TweenMgr.lerp_Num(nbtm, 180, t);\r\n        this.ClothOpenBtn.rotation = t.outParams[0][0];\r\n    }\r\n    RoteUp(t) {\r\n        let nbtm = this.ClothOpenBtn.rotation;\r\n        TweenMgr.lerp_Num(nbtm, 0, t);\r\n        this.ClothOpenBtn.rotation = t.outParams[0][0];\r\n    }\r\n    tweenbagUp(t) {\r\n        let nbtm = this.ShowView.bottom;\r\n        TweenMgr.lerp_Num(nbtm, 0, t);\r\n        this.ShowView.bottom = t.outParams[0][0];\r\n    }\r\n    tweenbagDown(t) {\r\n        let nbtm = this.ShowView.bottom;\r\n        TweenMgr.lerp_Num(nbtm, -240, t);\r\n        this.ShowView.bottom = t.outParams[0][0];\r\n    }\r\n    tweenbtnLeft(t) {\r\n        let nX = this.BtnBar.x;\r\n        TweenMgr.lerp_Num(nX, 75, t);\r\n        this.BtnBar.x = t.outParams[0][0];\r\n    }\r\n    tweenbtnRight(t) {\r\n        let nX = this.BtnBar.x;\r\n        TweenMgr.lerp_Num(nX, this.windowwidth - 75, t);\r\n        this.BtnBar.x = t.outParams[0][0];\r\n    }\r\n    onClick_Btnshortcut() {\r\n        UIMgr.show(\"UIMain\");\r\n    }\r\n}\r\n//# sourceMappingURL=UIReady.js.map",
