\n                break;\r\n        }\r\n    }\r\n    OnShow() {\r\n        this.data.ReportShow();\r\n    }\r\n    OnClick() {\r\n        this.data.Click();\r\n        if (this.onClick_ != null) {\r\n            this.onClick_(this);\r\n        }\r\n    }\r\n    onClick() {\r\n        this.OnClick();\r\n    }\r\n}\r\n//# sourceMappingURL=PromoItem.js.map",
